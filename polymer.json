{
  "entrypoint": "index.html",
  "shell": "src/cb-app.html",
  "fragments": [
    "src/cb-list.html",
    "src/cb-recipe.html",
    "src/cb-edit-recipe.html",
    "src/cb-offline.html",
    "src/cb-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "name": "es5-bundled",
    "bundle": true,
    "js": {"minify": true, "compile": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "addPushManifest": true
  },{
    "name": "es6-unbundled",
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "addPushManifest": true
  },{
    "name": "es6-bundled",
    "bundle": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true},
    "addServiceWorker": true,
    "addPushManifest": true
  }]
}
