{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "src",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/",
        "headers": [{
          "key": "Link",
          "value": "</components/firebase/firebase.js>;rel=preload;as=script;nopush,</components/redux/index.js>;rel=preload;as=script,</components/redux-thunk/index.js>;rel=preload;as=script"}]
      }
    ]
  }
}
